
numero1 = float(input("Digite um numero:"))
numero2 = float(input("Digite mais um numero:"))

soma = numero1 + numero2
sub = numero1 - numero2
multi = numero1 * numero2
div = numero1 / numero2

print("Operação de Soma:", soma)
print("Operação de Subtração:", sub)
print("Operação de Multiplicação:", multi)
print("Operação de Divisão:", div)
